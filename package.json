{
	"name": "self-zunda",
	"version": "1.0.0",
	"description": "ずんだもんのDiscord Bot なのだ！",
	"main": "dist/index.js",
	"type": "module",
	"scripts": {
		"dev": "vite-node --watch src/index.ts",
		"dev:validate": "concurrently \"npm run dev\" \"npm run type-check -- --watch\"",
		"deploy-commands": "tsx src/deploy-commands.ts",
		"start": "node dist/index.js",
		"build": "vite build",
		"test": "vitest run",
		"test:watch": "vitest",
		"type-check": "tsc --noEmit",
		"check": "biome check --write .",
		"format": "biome format --write .",
		"lint": "biome lint .",
		"validate": "npm run type-check && npm run check && npm run test",
		"generate-voicevox-api": "tsx src/cli/generate-voicevox-api.ts",
		"generate-voicevox-all": "tsx src/cli/generate-voicevox-all.ts",
		"generate-voicevox-all:custom": "cross-env VOICEVOX_URL=$VOICEVOX_URL tsx src/cli/generate-voicevox-all.ts"
	},
	"dependencies": {
		"@discordjs/voice": "^0.18.0",
		"axios": "^1.8.4",
		"discord.js": "^14.14.1",
		"dotenv": "^16.4.5"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@types/jest": "^29.5.14",
		"@types/node": "^20.11.24",
		"concurrently": "^8.2.2",
		"cross-env": "^7.0.3",
		"openapi-typescript": "^7.6.1",
		"openapi-typescript-codegen": "^0.25.0",
		"rimraf": "^5.0.5",
		"swagger-typescript-api": "^13.0.27",
		"tsx": "^4.19.3",
		"typescript": "^5.3.3",
		"vite": "^6.2.2",
		"vite-node": "^3.0.9",
		"vitest": "^3.0.9"
	}
}